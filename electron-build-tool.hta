				//Change Package name
				//************************************************************************************************
				Function changeFiles(package, name1, name2, nameBuild1, nameBuild2, autoupdate, strLine, chrome_version, chrome, strLineChrome, split_input_version_first, split_input_version_second, split_input_version_thurd)

					//read document
					//************************************************************************************************
						Set objFile = objFSO.OpenTextFile(package, 1)
							srtTextFTPPackage = objFile.ReadAll
						objFile.Close

					//change name
					//************************************************************************************************
						Set objFile = objFSO.OpenTextFile(package, 1)
							For i = 0 to 0
							    objFile.readLine
							Next
							srtLineFTPname = objFile.ReadLine
						objFile.Close

						
						Set objFile = objFSO.OpenTextFile(package, 2)
							concat_string_line_name = "	""name"": """ & name2 &""","
							srtTextFTPPackage = Replace(srtTextFTPPackage, srtLineFTPname, concat_string_line_name)
							objFile.write srtTextFTPPackage
						objFile.Close

					//change version
					//************************************************************************************************
						Set objFile = objFSO.OpenTextFile(package, 1)
							For i = 0 to 1
							    objFile.readLine
							Next
							srtLineFTPversion = objFile.ReadLine
						objFile.Close

						version_new_input = convertToString(split_input_version_first,split_input_version_second,split_input_version_thurd)
						Set objFile = objFSO.OpenTextFile(package, 2)
							concat_string_line_version = "  ""version"": """ & version_new_input &""","
							srtTextFTPPackage = Replace(srtTextFTPPackage, srtLineFTPversion, concat_string_line_version)
							objFile.write srtTextFTPPackage
						objFile.Close

					//change Build bit
					//************************************************************************************************
						Set objFile = objFSO.OpenTextFile(package, 1)
							For i = 0 to 6
							    objFile.readLine
							Next
							srtLineFTPbuild = objFile.ReadLine
						objFile.Close

						changeBuildFtp = "build  --win --" & nameBuild2 &""
						Set objFile = objFSO.OpenTextFile(package, 2)
							concat_string_line_build = "		""build"": """ & changeBuildFtp &""","
							srtTextFTPPackage = Replace(srtTextFTPPackage, srtLineFTPbuild, concat_string_line_build)
							objFile.write srtTextFTPPackage
						objFile.Close

					//change Build bit
					//************************************************************************************************
						Set objFile = objFSO.OpenTextFile(package, 1)
							For i = 0 to 14
							    objFile.readLine
							Next
							srtLineFTPremote = objFile.ReadLine
						objFile.Close

						changeFTPpathName = "http://appupdate.schoolriver.com/uploads/" & name2 &""
						Set objFile = objFSO.OpenTextFile(package, 2)
							concat_string_line_remote = "		  ""remoteReleases"": """ & changeFTPpathName &"/"""
							srtTextFTPPackage = Replace(srtTextFTPPackage, srtLineFTPremote, concat_string_line_remote)
							objFile.write srtTextFTPPackage
						objFile.Close
						// MsgBox srtLineFTPremote

					//CHANGE IN AUTOUPDATE FILE
					//************************************************************************************************
						//Autoupdate file
						Set objFile = objFSO.OpenTextFile(autoupdate, 1)
							strTextFTPautoupdate = objFile.ReadAll
						objFile.Close

						//change 
						changeFTPpathAuto = "		autoUpdater.setFeedURL('http://appupdate.schoolriver.com/uploads/" & name2 & "/')"
						Set objFile = objFSO.OpenTextFile(autoupdate, 1)
							For i = 0 to 12
							    objFile.readLine
							Next
							srtLineFTPauto = objFile.ReadLine
						objFile.Close

						Set objFile = objFSO.OpenTextFile(autoupdate, 2)
							strTextFTPAutoupdate = Replace(strTextFTPAutoupdate, srtLineFTPauto, changeFTPpathAuto)
							objFile.write strTextFTPAutoupdate
						objFile.Close	

				End Function
			</SCRIPT>
			<div>
				<FORM>
					<label>APX Kiosk</label>
					<div class="button">
						<div id="kiosk_progres" style="color: black;">Building Aplication...Please what this will take couple of minutes</div>
						<button id="apx_kiosk_32bit" NAME="apx_kiosk_32">32bit Build</button>
						<button id="apx_kiosk_64bit" NAME="apx_kiosk_64">64bit Build</button>

						<label>Current Version</label>
						<h5 id="version_kiosk" style="color:black;"></h5>

						<label>New Version</label>
						<input id="new_version_kiosk" value="" style="color:black;" type="text"/>

						<label>Chrome Version</label>
						<input id="chrome_version_kiosk" value="" style="color:black;" type="text"/> 
					</div>
					<SCRIPT LANGUAGE="VBScript">
							// get version from application
							Set objFSO = CreateObject("Scripting.FileSystemObject")

							Set objFile = objFSO.OpenTextFile("C:\Users\Master\Documents\APX-Kiosk-Electron\package.json", 1)
							For i = 1 to 2
							    objFile.readLine
							Next
							strLineKiosk = objFile.ReadLine

							splitLines = Split(strLineKiosk, " ")
							splitLinesG = splitLines(3)
							// app_version = Mid(splitLinesG, 2, 5)
							front_char_remove = Mid(splitLinesG, 2)
							app_version = Left(front_char_remove,Len(front_char_remove)-2)
							objFile.Close
							document.getelementbyid("version_kiosk").innerHTML = app_version

							//increment app version
							version_split = Split(app_version, ".")

							version_int_first_kiosk = CInt(version_split(0))
							version_int_second_kiosk = CInt(version_split(1))
							version_int_thurd_kiosk = CInt(version_split(2))

							If version_int_thurd_kiosk < 1000 THEN
								value3 = version_int_thurd_kiosk + 1
								version_new = convertToString(version_int_first_kiosk,version_int_second_kiosk,value3)
							Else
								If version_int_second_kiosk < 1000 THEN
									version_int_thurd_kiosk = 0
									value2 = version_int_second_kiosk + 1
									version_new = convertToString(version_int_first_kiosk,value2,version_int_thurd_kiosk)
								Else
									If version_int_first_kiosk < 1000 THEN
										version_int_second_kiosk = 0
										value1 = version_int_first_kiosk + 1
										version_new = convertToString(value1,version_int_second_kiosk,version_int_thurd_kiosk)
									End If
								End If
							End If
							document.getelementbyid("new_version_kiosk").value = version_new

							//Hide loader massage or whatever while building
							//=================================================================
								document.getelementbyid("kiosk_progres").style.visibility = "hidden"
							//=================================================================

							//Get version from Chrome file
							Set objFile = objFSO.OpenTextFile("C:\Users\Master\Documents\APX-Kiosk-Electron\electron-compatibility\chrome.js", 1)
								For i = 0 to 82
								    objFile.readLine
								Next
								strLineChrome = objFile.ReadLine

								splitLines = Split(strLineChrome, " ")

								splitLinesG = splitLines(2)
								front_char_remove = Mid(splitLinesG, 2)
								chrome_version = Left(front_char_remove,Len(front_char_remove)-2)
								document.getelementbyid("chrome_version_kiosk").value = chrome_version
							objFile.Close
					</SCRIPT>

					<SCRIPT FOR="apx_kiosk_32" EVENT="onClick" LANGUAGE="VBScript">
						Set objFSO = CreateObject("Scripting.FileSystemObject")
						Set WshShell = CreateObject("WScript.Shell")

						// Validate	version
						input_version_value = document.getelementbyid("new_version_kiosk").value
						split_input_version = Split(input_version_value, ".")
						lenght_of_split_input_version = UBound(split_input_version)

						If lenght_of_split_input_version = 2 THEN

							split_input_version_first = CInt(split_input_version(0))
							split_input_version_second = CInt(split_input_version(1))
							split_input_version_thurd = CInt(split_input_version(2))
							
							If ((split_input_version_thurd > version_int_thurd_kiosk) AND (split_input_version_second >= version_int_second_kiosk)) XOR((split_input_version_second > version_int_second_kiosk) AND (split_input_version_first >= version_int_first_kiosk)) XOR(split_input_version_first > version_int_first_kiosk) THEN

								//disable change version input after first build is initiated
								document.getelementbyid("new_version_kiosk").disabled = true
								document.getelementbyid("chrome_version_kiosk").disabled = true


								//TEST AREA
								//*************************************************************************************
									package = "C:\Users\Master\Documents\APX-Kiosk-Electron\package.json"
									name1 = "apx_kiosk_64"
									name2 = "apx_kiosk_32"
									nameBuild1 = "x64"
									nameBuild2 = "ia32"
									autoupdate = "C:\Users\Master\Documents\APX-Kiosk-Electron\autoupdate.js"
									strLine = strLineKiosk
									chrome_version = "chrome_version_kiosk"
									chrome = "C:\Users\Master\Documents\APX-Kiosk-Electron\electron-compatibility\chrome.js"

									call changeFiles(package,name1,name2,nameBuild1,nameBuild2,autoupdate,strLine,chrome_version,chrome,strLineChrome,split_input_version_first, split_input_version_second, split_input_version_thurd)
								//*************************************************************************************

								//DELETE FOLDER DIST
								//************************************************************************************************
									//delete existing DIST build folder
									dir = WshShell.ExpandEnvironmentStrings("C:\Users\Master\Documents\APX-Kiosk-Electron\dist")
									If objFSO.FolderExists(dir) Then
										blnAnswer = window.confirm("Would you like to delete the dist folder?") 
										If blnAnswer Then 
											objFSO.DeleteFolder dir
										    Msgbox "dist folder deleted"
										End If 
									Else
										alert("dist folder does not exist")
									End If

									//disable buttns while building
									//nnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnn
										disableButtons()
									//nnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnnn

									//Show loader massage or whatever while building
									//==================================================================
										document.getelementbyid("kiosk_progres").style.visibility = "visible"
